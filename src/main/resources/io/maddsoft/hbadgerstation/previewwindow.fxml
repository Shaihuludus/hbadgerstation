<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.glyphfont.Glyph?>

<VBox prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.maddsoft.hbadgerstation.gui.preview.PreviewWindowController">
   <children>
      <HBox fillHeight="false" spacing="5.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="zoomOutButton" mnemonicParsing="false" onAction="#zoomOut">
               <graphic>
                  <Glyph fontFamily="FontAwesome" icon="MINUS_CIRCLE" />
               </graphic>
            </Button>
            <Button fx:id="zoomInButton" mnemonicParsing="false" onAction="#zoomIn">
               <graphic>
                  <Glyph fontFamily="FontAwesome" icon="PLUS_CIRCLE" />
               </graphic>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </HBox>
      <StackPane VBox.vgrow="ALWAYS" fx:id="subSceneContainer"/>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
