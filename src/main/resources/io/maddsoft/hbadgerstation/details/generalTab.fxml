<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>

<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="io.maddsoft.hbadgerstation.gui.details.GeneralTabController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <children>
    <GridPane alignment="CENTER">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
        <Label prefHeight="20.0" prefWidth="212.0" text="Name" GridPane.halignment="RIGHT" />
        <TextField fx:id="nameField" editable="false" GridPane.columnIndex="1" />
        <Label prefHeight="20.0" prefWidth="212.0" text="Description" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
        <TextArea fx:id="descriptionField" editable="false" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label prefWidth="212.0" text="Path" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
        <TextField fx:id="directoryPathField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </children>
      <GridPane.margin>
        <Insets />
      </GridPane.margin>
    </GridPane>
  </children>
</GridPane>
